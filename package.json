{
  "name": "terios",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "dev": {
      "cache": false
    },
    "lint": {},
    "test": {}
  },
  "dependencies": {
    "@prisma/client": "5.20.0",
    "bcrypt": "^6.0.0",
    "env-cmd": "^11.0.0",
    "next": "16.0.1",
    "normalize.css": "^8.0.1",
    "prisma": "5.20.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "sass": "^1.94.0",
    "scss": "^0.2.4",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@prisma/client": "5.20.0",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "prettier": "^3.6.2",
    "prisma": "5.20.0",
    "turbo": "^2.6.1",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@4.11.0",
  "workspaces": [
    "apps/frontend/",
    "apps/backend/",
    "packages/*"
  ],
  "prisma": {
    "schema": "packages/prisma/schema.prisma"
  }
}
