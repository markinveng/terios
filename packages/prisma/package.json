{
  "name": "@terios/prisma",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@4.11.0",
  "main": "index.js",
  "scripts": {
    "generate": "env-cmd -f ../../.env prisma generate --schema schema.prisma",
    "migrate:dev": "env-cmd -f ../../.env prisma migrate dev --schema schema.prisma",
    "migrate:deploy": "env-cmd -f ../../.env prisma migrate deploy --schema schema.prisma",
    "studio": "env-cmd -f ../../.env prisma studio --schema schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "5.20.0",
    "env-cmd": "^11.0.0",
    "prisma": "5.20.0"
  }
}
